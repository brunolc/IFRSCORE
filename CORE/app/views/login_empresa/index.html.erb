<%= form_tag({action: 'login2'}, class: 'form-signin', method: 'post') do %>

    <h2 class="form-signin-heading">Login da Empresa</h2>
    <input type="text" name="cnpj"
        class="form-control" placeholder="Seu cnpj" required autofocus style="width: 258px;">
    <input type="password"  name="senha"
        class="form-control" placeholder="Sua senha" required style="width: 258px;">
    <button class="btn btn-lg btn-primary btn-block" type="submit" id="vai" style="width: 258px;">Login</button>

<% end %>
